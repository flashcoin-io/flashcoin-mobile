      "bitcoinjs-lib": "npm:bitcoinjs-lib@^2.3.0",
      "bs58check": "npm:bs58check@^1.2.0",
      "core-js": "npm:core-js@^2.4.1",
      "crypto-js": "npm:crypto-js@^3.1.8",
